<!-- Google tag (gtag.js) -->
<script async src="https://www.googletagmanager.com/gtag/js?id={{process.env.G_ANALYTICS}}"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', process.env.G_ANALYTICS);
</script>
{{#each classes}}
	<div class="col_4">
		<h4>{{title}}</h4>
		<p>{{description}}</p>
		<a class="button" href="/classes/{{_id}}/details">View Class</a>
	</div>
{{/each}}